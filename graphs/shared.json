{
  "$schema": "https://arkit.js.org/schema.json",
  "components": [
    {
      "type": "Entry",
      "patterns": [
        "src/shared.js"
      ]
    },
    {
      "type": "Component",
      "patterns": [
        "src/shared/*.js"
      ]
    }
  ],
  "output": {
    "path": "graphs/shared.svg",
    "groups": [
      {
        "type": "Shared",
        "patterns": ["src/shared/*.js"]
      }
    ]
  }
}
