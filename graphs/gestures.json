{
  "$schema": "https://arkit.js.org/schema.json",
  "components": [
    {
      "type": "Entry",
      "patterns": [
        "src/gestures.js"
      ]
    },
    {
      "type": "Component",
      "patterns": [
        "src/gestures/*.js"
      ]
    }
  ],
  "output": {
    "path": "graphs/gestures.svg",
    "groups": [
      {
        "type": "Gestures",
        "patterns": ["src/gestures/**/*.js"]
      }
    ]
  }
}
