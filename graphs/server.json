{
  "$schema": "https://arkit.js.org/schema.json",
  "components": [
    {
      "type": "Entry",
      "patterns": [
        "src/server.js"
      ]
    },
    {
      "type": "Component",
      "patterns": [
        "src/server/*.js"
      ]
    }
  ],
  "output": {
    "path": "graphs/server.svg",
    "groups": [
      {
        "type": "Server",
        "patterns": ["src/server/*.js"]
      }
    ]
  }
}
